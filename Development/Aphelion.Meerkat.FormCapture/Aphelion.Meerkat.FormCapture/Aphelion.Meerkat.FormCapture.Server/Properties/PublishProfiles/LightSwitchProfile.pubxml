<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="12.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>Package</WebPublishMethod>
    <UseLightSwitchWebDeploymentProvider>False</UseLightSwitchWebDeploymentProvider>
    <AuthenticationType>None</AuthenticationType>
    <RequiresIntrinsicDataSource>false</RequiresIntrinsicDataSource>
    <DeployDatabase>True</DeployDatabase>
    <LightSwitchPublishVersion>v2.2</LightSwitchPublishVersion>
    <PackageLocation>C:/Installs/Meerkat_FormCapture//Aphelion.Meerkat.FormCapture.zip</PackageLocation>
    <CleanWPPAllFilesInSingleFolder>False</CleanWPPAllFilesInSingleFolder>
    <ExcludeGeneratedDebugSymbol>true</ExcludeGeneratedDebugSymbol>
    <DisableAllVSGeneratedMSDeployParameter>True</DisableAllVSGeneratedMSDeployParameter>
    <AutoParameterizationWebConfigConnectionStrings>True</AutoParameterizationWebConfigConnectionStrings>
    <WPPAllFilesInSingleFolder>../bin/Debug/app.publish/</WPPAllFilesInSingleFolder>
    <PackageSourceManifest>../bin/Debug/Aphelion.Meerkat.FormCapture.xml</PackageSourceManifest>
    <ProjectParametersXMLFile>..//Aphelion.Meerkat.FormCapture.Server/generated.parameters.xml</ProjectParametersXMLFile>
    <LightSwitchSqlScriptFile>../bin/Debug/Aphelion.Meerkat.FormCapture.sql</LightSwitchSqlScriptFile>
  </PropertyGroup>
  <Target Name="BeforeBuild">
    <MSBuild Projects="../Aphelion.Meerkat.FormCapture.lsxproj" Targets="BeforeLightSwitchPublishApplication" />
  </Target>
</Project>