        IsOptional="True"
        ParameterType=":Int32?" />
      <QueryOperationParameter
        Name="ReportDueStartDate"
        IsOptional="True"
        ParameterType=":Date?" />
      <QueryOperationParameter
        Name="ReportDueEndDate"
        IsOptional="True"
        ParameterType=":Date?" />
      <QueryOperationParameter
        Name="CustomReportType"
        IsOptional="True"
        ParameterType=":Int32?" />
