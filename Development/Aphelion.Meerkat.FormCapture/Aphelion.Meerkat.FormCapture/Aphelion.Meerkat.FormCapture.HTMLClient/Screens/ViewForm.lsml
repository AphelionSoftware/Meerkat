            Kind="Group"
            View="Microsoft.LightSwitch.MobileWeb:RowsLayout">
            <ContentItem
              Name="Form_isConfidential"
              Kind="Value"
              DataType=":Boolean"
              View="Microsoft.LightSwitch.MobileWeb:FlipSwitchControl">
