<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <IncludeCompositeObjects>True</IncludeCompositeObjects>
    <TargetDatabaseName>Meerkat</TargetDatabaseName>
    <DeployScriptFileName>Meerkat.sql</DeployScriptFileName>
    <TargetConnectionString>Data Source=(localdb)\v11.0;Integrated Security=True;Pooling=False</TargetConnectionString>
    <ScriptDatabaseOptions>True</ScriptDatabaseOptions>
    <BlockOnPossibleDataLoss>True</BlockOnPossibleDataLoss>
    <BackupDatabaseBeforeChanges>False</BackupDatabaseBeforeChanges>
    <ProfileVersionNumber>1</ProfileVersionNumber>
    <CreateNewDatabase>True</CreateNewDatabase>
  </PropertyGroup>
</Project>