      ReturnType="CustomReport_Calendar*">
      <QueryOperation.FilterExpression>
        <QueryFilterExpressionTree>
          <BinaryExpression
            NodeType="And">
            <BinaryExpression.Left>
              <BinaryExpression
                NodeType="And">
                <BinaryExpression.Left>
                  <BinaryExpression
                    NodeType="And">
                    <BinaryExpression.Left>
                      <BinaryExpression
                        NodeType="Equal">
                        <BinaryExpression.Left>
                          <ChainExpression>
                            <MemberExpression
                              Member="CustomReport" />
                            <MemberExpression
                              Member="CustomReport_ID" />
                          </ChainExpression>
                        </BinaryExpression.Left>
                        <BinaryExpression.Right>
                          <ParameterExpression
                            Parameter="CustomReport" />
                        </BinaryExpression.Right>
                      </BinaryExpression>
                    </BinaryExpression.Left>
                    <BinaryExpression.Right>
                      <TernaryExpression
                        NodeType="Between">
                        <TernaryExpression.First>
                          <ChainExpression>
                            <MemberExpression
                              Member="ReportDueDate" />
                          </ChainExpression>
                        </TernaryExpression.First>
                        <TernaryExpression.Second>
                          <ParameterExpression
                            Parameter="ReportDueStartDate" />
                        </TernaryExpression.Second>
                        <TernaryExpression.Third>
                          <ParameterExpression
                            Parameter="ReportDueEndDate" />
                        </TernaryExpression.Third>
                      </TernaryExpression>
                    </BinaryExpression.Right>
                  </BinaryExpression>
                </BinaryExpression.Left>
                <BinaryExpression.Right>
                  <BinaryExpression
                    NodeType="GreaterThanOrEqual">
                    <BinaryExpression.Left>
                      <ChainExpression>
                        <MemberExpression
                          Member="ReportDueDate" />
                      </ChainExpression>
                    </BinaryExpression.Left>
                    <BinaryExpression.Right>
                      <CallExpression
                        Target=":RelativeDates/GlobalValues[Today]" />
                    </BinaryExpression.Right>
                  </BinaryExpression>
                </BinaryExpression.Right>
              </BinaryExpression>
            </BinaryExpression.Left>
            <BinaryExpression.Right>
              <BinaryExpression
