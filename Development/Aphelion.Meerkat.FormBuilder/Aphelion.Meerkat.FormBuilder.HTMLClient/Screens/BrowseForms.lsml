                  <ScreenExpressionTree>
                    <ChainExpression>
                      <CallExpression
                        Target="!module/Methods[showAddEditForm]">
                        <CallExpression.Attributes>
                          <AssociatedCollection
                            Collection="BrowseForms/Properties[FormsSorted]" />
                        </CallExpression.Attributes>
                        <ArgumentExpression
                          Parameter="Form">
                          <ChainExpression>
                            <MemberExpression
